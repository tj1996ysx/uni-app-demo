
.body{
}
.tab-body{
	height: 50px;
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 200;
}
.tab{
	display: -webkit-box;
	display: flex;
	display: -webkit-flex;
	-webkit-box-pack:center;
	-webkit-justify-content:center;
	        justify-content:center;
	-webkit-box-align:center;
	-webkit-align-items:center;
	        align-items:center;
	height: 100%;
	/* background-color: #000000; */
	color: #FFFFFF;
	font-size: 15px;
}
.tab-box {
	text-align: center;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.icon-box {
	width: 60%;
	height: 30px;
	background: #FFFFFF;
	color: #000000;
	line-height: 30px;
	margin: 7.5px 20%;
	border-radius: 5px;
}

